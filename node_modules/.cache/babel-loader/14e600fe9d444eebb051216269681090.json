{"ast":null,"code":"/* eslint-disable */\nimport { defineComponent, ref } from 'vue';\nimport { useRouter } from 'vue-router';\nexport default defineComponent({\n  setup: function setup() {\n    var _this = this;\n\n    var router = useRouter();\n    var searchDropdown = ref(false);\n\n    var showSearchDropdown = function showSearchDropdown() {\n      searchDropdown.value = true;\n    };\n\n    var loginpushpage = function loginpushpage() {\n      localStorage.removeItem('token');\n      router.push('/login');\n    };\n\n    var hideSearchDropdown = function hideSearchDropdown() {\n      searchDropdown.value = false;\n    };\n\n    var uzLang = function uzLang() {\n      localStorage.setItem(\"lang\", \"uz\");\n\n      _this.$router.reload();\n    };\n\n    var enLang = function enLang() {\n      localStorage.setItem(\"lang\", \"en\");\n    };\n\n    return {\n      searchDropdown: searchDropdown,\n      showSearchDropdown: showSearchDropdown,\n      hideSearchDropdown: hideSearchDropdown,\n      loginpushpage: loginpushpage,\n      uzLang: uzLang,\n      enLang: enLang\n    };\n  }\n});","map":{"version":3,"sources":["D:\\Apteka Vue\\Pharmacy-main\\Pharmacy-main\\src\\components\\top-bar\\Main.vue"],"names":[],"mappings":"AA2JA;AACA,SAAS,eAAT,EAA0B,GAA1B,QAAqC,KAArC;AACA,SAAS,SAAT,QAA0B,YAA1B;AAEA,eAAe,eAAe,CAAC;AAC7B,EAAA,KAD6B,mBACrB;AAAA;;AACN,QAAM,MAAK,GAAI,SAAS,EAAxB;AACA,QAAM,cAAa,GAAI,GAAG,CAAC,KAAD,CAA1B;;AAEA,QAAM,kBAAiB,GAAI,SAArB,kBAAqB,GAAM;AAC/B,MAAA,cAAc,CAAC,KAAf,GAAuB,IAAvB;AACF,KAFA;;AAGA,QAAM,aAAY,GAAI,SAAhB,aAAgB,GAAM;AAC1B,MAAA,YAAY,CAAC,UAAb,CAAwB,OAAxB;AACA,MAAA,MAAM,CAAC,IAAP,CAAY,QAAZ;AACF,KAHA;;AAKA,QAAM,kBAAiB,GAAI,SAArB,kBAAqB,GAAM;AAC/B,MAAA,cAAc,CAAC,KAAf,GAAuB,KAAvB;AACF,KAFA;;AAIA,QAAM,MAAK,GAAI,SAAT,MAAS,GAAM;AACnB,MAAA,YAAY,CAAC,OAAb,CAAqB,MAArB,EAA6B,IAA7B;;AACA,MAAA,KAAI,CAAC,OAAL,CAAa,MAAb;AACF,KAHA;;AAIA,QAAM,MAAK,GAAI,SAAT,MAAS,GAAM;AACnB,MAAA,YAAY,CAAC,OAAb,CAAqB,MAArB,EAA4B,IAA5B;AACF,KAFA;;AAIA,WAAO;AACL,MAAA,cAAc,EAAd,cADK