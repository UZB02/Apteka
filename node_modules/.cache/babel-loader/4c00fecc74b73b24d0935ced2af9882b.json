{"ast":null,"code":"import { defineComponent, computed, onMounted, ref, watch } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport { useStore } from '@/store';\nimport { helper as $h } from '@/utils/helper';\nimport { activeMobileMenu, toggleMobileMenu, linkTo, enter, leave } from './index';\nimport { nestedMenu } from '@/layouts/side-menu';\nexport default defineComponent({\n  setup: function setup() {\n    var route = useRoute();\n    var router = useRouter();\n    var store = useStore();\n    var formattedMenu = ref([]);\n    var mobileMenu = computed(function () {\n      return nestedMenu(store.state.sideMenu.menu, route);\n    });\n    watch(computed(function () {\n      return route.path;\n    }), function () {\n      formattedMenu.value = $h.toRaw(mobileMenu.value);\n    });\n    onMounted(function () {\n      formattedMenu.value = $h.toRaw(mobileMenu.value);\n    });\n    return {\n      activeMobileMenu: activeMobileMenu,\n      toggleMobileMenu: toggleMobileMenu,\n      formattedMenu: formattedMenu,\n      router: router,\n      linkTo: linkTo,\n      enter: enter,\n      leave: leave\n    };\n  }\n});","map":{"version":3,"sources":["D:\\Apteka Vue\\Pharmacy-main\\Pharmacy-main\\src\\components\\mobile-menu\\Main.vue"],"names":[],"mappings":"AAoHA,SAAS,eAAT,EAA0B,QAA1B,EAAoC,SAApC,EAA+C,GAA/C,EAAoD,KAApD,QAAiE,KAAjE;AACA,SAAS,QAAT,EAAmB,SAAnB,QAAoC,YAApC;AACA,SAAS,QAAT,QAAyB,SAAzB;AACA,SAAS,MAAK,IAAK,EAAnB,QAA6B,gBAA7B;AACA,SACE,gBADF,EAEE,gBAFF,EAGE,MAHF,EAIE,KAJF,EAKE,KALF,QAMO,SANP;AAOA,SAAS,UAAT,QAA2B,qBAA3B;AAEA,eAAe,eAAe,CAAC;AAC7B,EAAA,KAD6B,mBACrB;AACN,QAAM,KAAI,GAAI,QAAQ,EAAtB;AACA,QAAM,MAAK,GAAI,SAAS,EAAxB;AACA,QAAM,KAAI,GAAI,QAAQ,EAAtB;AACA,QAAM,aAAY,GAAI,GAAG,CAAC,EAAD,CAAzB;AACA,QAAM,UAAS,GAAI,QAAQ,CAAC;AAAA,aAC1B,UAAU,CAAC,KAAK,CAAC,KAAN,CAAY,QAAZ,CAAqB,IAAtB,EAA4B,KAA5B,CADgB;AAAA,KAAD,CAA3B;AAIA,IAAA,KAAK,CACH,QAAQ,CAAC;AAAA,aAAM,KAAK,CAAC,IAAZ;AAAA,KAAD,CADL,EAEH,YAAM;AACJ,MAAA,aAAa,CAAC,KAAd,GAAsB,EAAE,CAAC,KAAH,CAAS,UAAU,CAAC,KAApB,CAAtB;AACF,KAJG,CAAL;AAOA,IAAA,SAAS,CAAC,YAAM;AACd,MAAA,aAAa,CAAC,KAAd,GAAsB,EAAE,CAAC,KAAH,CAAS,UAAU,CAAC,KAApB,CAAtB;AACD,KAFQ,CAAT;AAIA,WAAO;AACL,MAAA,gBAAgB,EAAhB,gBADK;AAEL,MAAA,gBAAgB,EAAhB,gBAFK;AAGL,MAAA,aAAa,EAAb,aAHK;AAIL,MAAA,MAAM,EAAN,MAJK;AAKL,MAAA,MAAM,EAAN,MALK;AAML,MAAA,KAAK,EAAL,KANK;AAOL,MAAA,KAAI,EAAJ;AAPK,KAAP;AASF;AA9B6B,CAAD,CAA9B","sourcesContent":["<template>\n  <!-- BEGIN: Mobile Menu -->\n  <div class=\"mobile-menu md:hidden\">\n    <div class=\"mobile-menu-bar\">\n      <a href class=\"flex mr-auto\">\n        <img\n          alt=\"Icewall Tailwind HTML Admin Template\"\n          class=\"w-6\"\n          src=\"@/assets/images/logo.svg\"\n        />\n      </a>\n      <BarChart2Icon\n        class=\"w-8 h-8 text-white transform -rotate-90\"\n        @click=\"toggleMobileMenu\"\n      />\n    </div>\n    <transition @enter=\"enter\" @leave=\"leave\">\n      <ul v-if=\"activeMobileMenu\" class=\"border-t border-theme-29 py-5\">\n        <!-- BEGIN: First Child -->\n        <template v-for=\"(menu, menuKey) in formattedMenu\">\n          <li\n            v-if=\"menu == 'devider'\"\n            :key=\"menu + menuKey\"\n            class=\"menu__devider my-6\"\n          ></li>\n          <li v-else :key=\"menu + menuKey\">\n            <a\n              href=\"javascript:;\"\n              class=\"menu\"\n              :class=\"{\n                'menu--active': menu.active,\n                'menu--open': menu.activeDropdown\n              }\"\n              @click=\"linkTo(menu, router)\"\n            >\n              <div class=\"menu__icon\">\n                <component :is=\"menu.icon\" />\n              </div>\n              <div class=\"menu__title\">\n                {{ menu.title }}\n                <div\n                  v-if=\"menu.subMenu\"\n                  class=\"menu__sub-icon\"\n                  :class=\"{ 'transform rotate-180': menu.activeDropdown }\"\n                >\n                  <ChevronDownIcon />\n                </div>\n              </div>\n            </a>\n            <!-- BEGIN: Second Child -->\n            <transition @enter=\"enter\" @leave=\"leave\">\n              <ul v-if=\"menu.subMenu && menu.activeDropdown\">\n                <li\n                  v-for=\"(subMenu, subMenuKey) in menu.subMenu\"\n                  :key=\"subMenuKey\"\n                >\n                  <a\n                    href=\"javascript:;\"\n                    class=\"menu\"\n                    :class=\"{ 'menu--active': subMenu.active }\"\n                    @click=\"linkTo(subMenu, router)\"\n                  >\n                    <div class=\"menu__icon\">\n                      <ActivityIcon />\n                    </div>\n                    <div class=\"menu__title\">\n                      {{ subMenu.title }}\n                      <div\n                        v-if=\"subMenu.subMenu\"\n                        class=\"menu__sub-icon\"\n                        :class=\"{\n                          'transform rotate-180': subMenu.activeDropdown\n                        }\"\n                      >\n                        <ChevronDownIcon />\n                      </div>\n                    </div>\n                  </a>\n                  <!-- BEGIN: Third Child -->\n                  <transition @enter=\"enter\" @leave=\"leave\">\n                    <ul v-if=\"subMenu.subMenu && subMenu.activeDropdown\">\n                      <li\n                        v-for=\"(lastSubMenu, lastSubMenuKey) in subMenu.subMenu\"\n                        :key=\"lastSubMenuKey\"\n                      >\n                        <a\n                          href=\"javascript:;\"\n                          class=\"menu\"\n                          :class=\"{ 'menu--active': lastSubMenu.active }\"\n                          @click=\"linkTo(lastSubMenu, router)\"\n                        >\n                          <div class=\"menu__icon\">\n                            <ZapIcon />\n                          </div>\n                          <div class=\"menu__title\">\n                            {{ lastSubMenu.title }}\n                          </div>\n                        </a>\n                      </li>\n                    </ul>\n                  </transition>\n                  <!-- END: Third Child -->\n                </li>\n              </ul>\n            </transition>\n            <!-- END: Second Child -->\n          </li>\n        </template>\n        <!-- END: First Child -->\n      </ul>\n    </transition>\n  </div>\n  <!-- END: Mobile Menu -->\n</template>\n\n<script>\nimport { defineComponent, computed, onMounted, ref, watch } from 'vue'\nimport { useRoute, useRouter } from 'vue-router'\nimport { useStore } from '@/store'\nimport { helper as $h } from '@/utils/helper'\nimport {\n  activeMobileMenu,\n  toggleMobileMenu,\n  linkTo,\n  enter,\n  leave\n} from './index'\nimport { nestedMenu } from '@/layouts/side-menu'\n\nexport default defineComponent({\n  setup() {\n    const route = useRoute()\n    const router = useRouter()\n    const store = useStore()\n    const formattedMenu = ref([])\n    const mobileMenu = computed(() =>\n      nestedMenu(store.state.sideMenu.menu, route)\n    )\n\n    watch(\n      computed(() => route.path),\n      () => {\n        formattedMenu.value = $h.toRaw(mobileMenu.value)\n      }\n    )\n\n    onMounted(() => {\n      formattedMenu.value = $h.toRaw(mobileMenu.value)\n    })\n\n    return {\n      activeMobileMenu,\n      toggleMobileMenu,\n      formattedMenu,\n      router,\n      linkTo,\n      enter,\n      leave\n    }\n  }\n})\n</script>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}