{"ast":null,"code":"import \"core-js/modules/es.array.find.js\";\nimport \"core-js/modules/es.function.name.js\";\n\n/* eslint-disable */\nimport { defineComponent, ref } from 'vue';\nimport { useRouter } from 'vue-router';\nexport default defineComponent({\n  setup: function setup() {\n    var router = useRouter();\n    var searchDropdown = ref(false);\n\n    var showSearchDropdown = function showSearchDropdown() {\n      searchDropdown.value = true;\n    };\n\n    var loginpushpage = function loginpushpage() {\n      localStorage.removeItem('token');\n      router.push('/login');\n    };\n\n    var hideSearchDropdown = function hideSearchDropdown() {\n      searchDropdown.value = false;\n    }; // const uzLang = () => { \n    //   localStorage.setItem(\"lang\", \"UZ\")\n    //   location.reload();\n    // }\n    // const enLang = () => { \n    //   localStorage.setItem(\"lang\", \"EN\")\n    //   location.reload();\n    // }\n    // const ruLang = () => { \n    //   localStorage.setItem(\"lang\", \"RU\")\n    //   location.reload();\n    // }\n\n\n    return {\n      searchDropdown: searchDropdown,\n      showSearchDropdown: showSearchDropdown,\n      hideSearchDropdown: hideSearchDropdown,\n      loginpushpage: loginpushpage // uzLang,\n      // enLang,\n      // ruLang\n\n    };\n  },\n  data: function data() {\n    return {\n      data: {\n        active: localStorage.getItem(\"lang\") || \"uz\",\n        flag: [{\n          name: \"ru\",\n          img: \"https://wikiflag.ru/wp-content/uploads/2022/03/1.jpg\"\n        }, {\n          name: \"en\",\n          img: \"https://media.baamboozle.com/uploads/images/41367/1593085639_60643\"\n        }, {\n          name: \"uz\",\n          img: \"https://i.pinimg.com/originals/31/59/7f/31597f5024d357ed5cbac66f4bd61d3b.png\"\n        }]\n      },\n      showLangItem: false,\n      modal: true,\n      selectedFlag: {}\n    };\n  },\n  mounted: function mounted() {\n    var _this = this;\n\n    this.selectedFlag = this.data.flag.find(function (flag) {\n      return flag.name === _this.data.active;\n    });\n  },\n  methods: {\n    toggleLangItem: function toggleLangItem() {\n      this.modal = false;\n      this.showLangItem = !this.showLangItem;\n    },\n    changeLang: function changeLang(name) {\n      this.data.active = name;\n      localStorage.setItem(\"lang\", name);\n      this.selectedFlag = this.data.flag.find(function (flag) {\n        return flag.name === name;\n      });\n      this.showLangItem = false;\n      this.modal = true;\n      location.reload();\n    }\n  }\n});","map":{"version":3,"sources":["D:\\Apteka Vue\\Pharmacy-main\\Pharmacy-main\\src\\components\\top-bar\\Main.vue"],"names":[],"mappings":";;;AAsKA;AACA,SAAS,eAAT,EAA0B,GAA1B,QAAqC,KAArC;AACA,SAAS,SAAT,QAA0B,YAA1B;AAEA,eAAe,eAAe,CAAC;AAC7B,EAAA,KAD6B,mBACrB;AACN,QAAM,MAAK,GAAI,SAAS,EAAxB;AACA,QAAM,cAAa,GAAI,GAAG,CAAC,KAAD,CAA1B;;AAEA,QAAM,kBAAiB,GAAI,SAArB,kBAAqB,GAAM;AAC/B,MAAA,cAAc,CAAC,KAAf,GAAuB,IAAvB;AACF,KAFA;;AAGA,QAAM,aAAY,GAAI,SAAhB,aAAgB,GAAM;AAC1B,MAAA,YAAY,CAAC,UAAb,CAAwB,OAAxB;AACA,MAAA,MAAM,CAAC,IAAP,CAAY,QAAZ;AACF,KAHA;;AAKA,QAAM,kBAAiB,GAAI,SAArB,kBAAqB,GAAM;AAC/B,MAAA,cAAc,CAAC,KAAf,GAAuB,KAAvB;AACF,KAFA,CAZM,CAgBN;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;;AAEA,WAAO;AACL,MAAA,cAAc,EAAd,cADK;AAEL,MAAA,kBAAkB,EAAlB,kBAFK;AAGL,MAAA,kBAAkB,EAAlB,kBAHK;AAIL,MAAA,aAAa,EAAb,aAJK,CAKL;AACA;AACA;;AAPK,KAAP;AASD,GAvC4B;AAwC7B,EAAA,IAxC6B,kBAwCtB;AACL,WAAO;AACL,MAAA,IAAI,EAAE;AACJ,QAAA,MAAM,EAAE,YAAY,CAAC,OAAb,CAAqB,MAArB,KAAgC,IADpC;AAEJ,QAAA,IAAI,EAAE,CACJ;AACE,UAAA,IAAI,EAAE,IADR;AAEE,UAAA,GAAG,EAAE;AAFP,SADI,EAKJ;AACE,UAAA,IAAI,EAAE,IADR;AAEE,UAAA,GAAG,EAAE;AAFP,SALI,EASJ;AACE,UAAA,IAAI,EAAE,IADR;AAEE,UAAA,GAAG,EAAE;AAFP,SATI;AAFF,OADD;AAkBL,MAAA,YAAY,EAAE,KAlBT;AAmBL,MAAA,KAAK,EAAE,IAnBF;AAoBL,MAAA,YAAY,EAAE;AApBT,KAAP;AAsBD,GA/D4B;AAgE7B,EAAA,OAhE6B,qBAgEnB;AAAA;;AACR,SAAK,YAAL,GAAoB,KAAK,IAAL,CAAU,IAAV,CAAe,IAAf,CAAoB,UAAC,IAAD;AAAA,aAAU,IAAI,CAAC,IAAL,KAAc,KAAI,CAAC,IAAL,CA