{"ast":null,"code":"import { defineComponent, onMounted, computed } from 'vue';\nimport { useStore } from '@/store';\nexport default defineComponent({\n  setup: function setup() {\n    var store = useStore();\n    var darkMode = computed(function () {\n      return store.state.main.darkMode;\n    });\n\n    var setDarkModeClass = function setDarkModeClass() {\n      darkMode.value ? cash('html').addClass('dark') : cash('html').removeClass('dark');\n    };\n\n    var switchMode = function switchMode() {\n      store.dispatch('main/setDarkMode', !darkMode.value);\n      setDarkModeClass();\n    };\n\n    onMounted(function () {\n      setDarkModeClass();\n    });\n    return {\n      switchMode: switchMode,\n      darkMode: darkMode\n    };\n  }\n});","map":{"version":3,"sources":["D:\\Apteka Vue\\Pharmacy-main\\Pharmacy-main\\src\\components\\dark-mode-switcher\\Main.vue"],"names":[],"mappings":"AAgBA,SAAS,eAAT,EAA0B,SAA1B,EAAqC,QAArC,QAAqD,KAArD;AACA,SAAS,QAAT,QAAyB,SAAzB;AAEA,eAAe,eAAe,CAAC;AAC7B,EAAA,KAD6B,mBACrB;AACN,QAAM,KAAI,GAAI,QAAQ,EAAtB;AACA,QAAM,QAAO,GAAI,QAAQ,CAAC;AAAA,aAAM,KAAK,CAAC,KAAN,CAAY,IAAZ,CAAiB,QAAvB;AAAA,KAAD,CAAzB;;AAEA,QAAM,gBAAe,GAAI,SAAnB,gBAAmB,GAAM;AAC7B,MAAA,QAAQ,CAAC,KAAT,GACI,IAAI,CAAC,MAAD,CAAJ,CAAa,QAAb,CAAsB,MAAtB,CADJ,GAEI,IAAI,CAAC,MAAD,CAAJ,CAAa,WAAb,CAAyB,MAAzB,CAFJ;AAGF,KAJA;;AAMA,QAAM,UAAS,GAAI,SAAb,UAAa,GAAM;AACvB,MAAA,KAAK,CAAC,QAAN,CAAe,kBAAf,EAAmC,CAAC,QAAQ,CAAC,KAA7C;AACA,MAAA,gBAAgB;AAClB,KAHA;;AAKA,IAAA,SAAS,CAAC,YAAM;AACd,MAAA,gBAAgB;AACjB,KAFQ,CAAT;AAIA,WAAO;AACL,MAAA,UAAU,EAAV,UADK;AAEL,MAAA,QAAO,EAAP;AAFK,KAAP;AAIF;AAxB6B,CAAD,CAA9B","sourcesContent":["<template>\n  <!-- BEGIN: Dark Mode Switcher-->\n  <div\n    class=\"dark-mode-switcher cursor-pointer shadow-md fixed bottom-0 right-0 box dark:bg-dark-2 border rounded-full w-40 h-12 flex items-center justify-center z-50 mb-10 mr-10\"\n    @click=\"switchMode\"\n  >\n    <div class=\"mr-4 text-gray-700 dark:text-gray-300\">Dark Mode</div>\n    <div\n      :class=\"{ 'dark-mode-switcher__toggle--active': darkMode }\"\n      class=\"dark-mode-switcher__toggle border\"\n    ></div>\n  </div>\n  <!-- END: Dark Mode Switcher-->\n</template>\n\n<script>\nimport { defineComponent, onMounted, computed } from 'vue'\nimport { useStore } from '@/store'\n\nexport default defineComponent({\n  setup() {\n    const store = useStore()\n    const darkMode = computed(() => store.state.main.darkMode)\n\n    const setDarkModeClass = () => {\n      darkMode.value\n        ? cash('html').addClass('dark')\n        : cash('html').removeClass('dark')\n    }\n\n    const switchMode = () => {\n      store.dispatch('main/setDarkMode', !darkMode.value)\n      setDarkModeClass()\n    }\n\n    onMounted(() => {\n      setDarkModeClass()\n    })\n\n    return {\n      switchMode,\n      darkMode\n    }\n  }\n})\n</script>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}