ore = useStore()\n    const darkMode = computed(() => store.state.main.darkMode)\n\n    const setDarkModeClass = () => {\n      darkMode.value\n        ? cash('html').addClass('dark')\n        : cash('html').removeClass('dark')\n    }\n\n    const switchMode = () => {\n      store.dispatch('main/setDarkMode', !darkMode.value)\n      setDarkModeClass()\n    }\n\n    onMounted(() => {\n      setDarkModeClass()\n    })\n\n    return {\n      switchMode,\n      darkMode\n    }\n  }\n})\n</script>\n"]}]}