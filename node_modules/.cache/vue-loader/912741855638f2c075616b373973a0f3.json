a-target=\"#header-footer-modal-preview\"\n          class=\"btn btn-primary\"\n          >{{ $t(\"add_new_product\") }}</a\n        >\n        <div class=\"dropdown\">\n          <button\n            class=\"dropdown-toggle btn px-2 box text-gray-700 dark:text-gray-300\"\n            aria-expanded=\"false\"\n          >\n            <span class=\"w-5 h-5 flex items-center justify-center\">\n              <PlusIcon class=\"w-4 h-4\" />\n            </span>\n          </button>\n          <div class=\"dropdown-menu w-40\">\n            <div class=\"dropdown-menu__content box dark:bg-dark-1 p-2\">\n              <a\n                href=\"\"\n                class=\"flex items-center block p-2 transition duration-300 ease-in-out bg-white dark:bg-dark-1 hover:bg-gray-200 dark:hover:bg-dark-2 rounded-md\"\n              >\n                <PrinterIcon class=\"w-4 h-4 mr-2\" /> {{ $t(\"print\") }}\n              </a>\n              <a\n                href=\"\"\n                class=\"flex items-center block p-2 transition duration-300 ease-in-out bg-white dark:bg-dark-1 hover:bg-gray-200 dark:hover:bg-dark-2 rounded-md\"\n              >\n                <FileTextIcon class=\"w-4 h-4 mr-2\" /> {{ $t(\"export_to_excel\") }}\n              </a>\n              <a\n                href=\"\"\n                class=\"flex items-center block p-2 transition duration-300 ease-in-out bg-white dark:bg-dark-1 hover:bg-gray-200 dark:hover:bg-dark-2 rounded-md\"\n              >\n                <FileTextIcon class=\"w-4 h-4 mr-2\" /> {{ $t(\"export_to_PDF\") }}\n              </a>\n            </div>\n          </div>\n        </div>\n        <div class=\"hidden md:block mx-auto text-gray-600\">\n          <!-- Showing 1 to 10 of 150 entries -->\n        </div>\n        <div class=\"w-full sm:w-auto mt-3 sm:mt-0 sm:ml-auto md:ml-0\">\n          <div class=\"w-56 relative text-gray-700 dark:text-gray-300\">\n            <input\n              type=\"text\"\n              class=\"form-control w-56 box pr-10 placeholder-theme-13\"\n              placeholder=\"Search...\"\n              v-model=\"searchKeyword\"\n              @input=\"searchData()\"\n            />\n            <SearchIcon\n              class=\"w-4 h-4 absolute my-auto inset-y-0 mr-3 right-0\"\n            />\n          </div>\n        </div>\n      </div>\n      <!-- BEGIN: Data List -->\n      <div class=\"intro-y col-span-12 overflow-auto lg:overflow-visible\">\n        <table class=\"table table-report -mt-2\">\n          <thead>\n            <tr>\n              <th\n                class=\"whitespace-nowrap\"\n                style=\"display: flex;align-items: center; justify-content: center;transform: translate(-10px)\"\n              >\n                {{ $t(\"id\") }}\n              </th>\n              <th class=\"whitespace-nowrap uppercase\">{{ $t(\"product\") }}</th>\n              <th class=\"whitespace-nowrap uppercase\">{{ $t(\"category\") }}</th>\n              <th class=\"text-center whitespace-nowrap uppercase\">{{ $t(\"price\") }}</th>\n              <th class=\"text-center whitespace-nowrap uppercase\">{{ $t(\"amount\") }}</th>\n              <th class=\"text-center whitespace-nowrap uppercase\">{{ $t(\"barcode\") }}</th>\n              <th class=\"text-center whitespace-nowrap uppercase\">{{ $t(\"date\") }}</th>\n              <th class=\"text-center whitespace-nowrap uppercase\">{{ $t(\"manufacturer\") }}</th>\n              <th class=\"text-center whitespace-nowrap uppercase\">{{ $t(\"actions\") }}</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr\n              v-for=\"(faker, fakerKey) in data.value\"\n              :key=\"fakerKey\"\n              class=\"intro-x\"\n            >\n              <td class=\"w-20\">\n                <div class=\"flex  justify-center\">\n                  <div\n                    class=\"w-10 h-10 image-fi -ml-5\"\n                    style=\"display: flex; align-items: center;justify-content