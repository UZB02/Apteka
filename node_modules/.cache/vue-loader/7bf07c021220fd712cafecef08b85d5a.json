n  props: {\n    options: {\n      type: Object,\n      default: () => ({})\n    },\n    refKey: {\n      type: String,\n      default: null\n    }\n  },\n  setup(props, context) {\n    const fileUploadRef = ref()\n    const bindInstance = () => {\n      if (props.refKey) {\n        const bind = inject(`bind[${props.refKey}]`)\n        if (bind) {\n          bind(fileUploadRef.value)\n        }\n      }\n    }\n\n    onMounted(() => {\n      bindInstance()\n    })\n\n    return {\n      props,\n      ...context,\n      fileUploadRef\n    }\n  }\n})\n</script>\n"]}]}